# AI Mental Chatbot WebUI - Environment Configuration
# Copy this file to .env and adjust values as needed

# Frontend port (default: 3001)
FRONTEND_PORT=3001

# Nginx port (default: 80)
NGINX_PORT=80

# Backend API URL (REQUIRED for chat to work)
# Use full host only (no trailing /api), e.g. http://localhost:3000
# Leave empty to use same-origin and rely on reverse proxy (/api -> backend)
VITE_API_URL=
